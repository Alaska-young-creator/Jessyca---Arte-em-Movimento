<!-- Seção de Áudio Trance -->
<section id="audio" class="container">
  <h2>Trance Music</h2>
  <audio id="trancePlayer" controls>
    <source src="https://example.com/audio/trance1.mp3" type="audio/mp3">
    Seu navegador não suporta a tag de áudio.
  </audio>
  <button onclick="playNextTrack()">Próxima Música</button>
</section>

<script>
  var audioFiles = [
    'https://example.com/audio/trance1.mp3',
    'https://example.com/audio/trance2.mp3',
    'https://example.com/audio/trance3.mp3'
  ];

  var currentTrack = 0;
  var audioPlayer = document.getElementById('trancePlayer');

  function playNextTrack() {
    currentTrack = (currentTrack + 1) % audioFiles.length;
    audioPlayer.src = audioFiles[currentTrack];
    audioPlayer.play();
  }
